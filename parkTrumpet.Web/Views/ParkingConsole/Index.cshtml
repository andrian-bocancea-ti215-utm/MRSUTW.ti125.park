@using parkTrumpet.Web.Models
@model ParkingConsoleModel
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

@using (Html.BeginForm("Index", "ParkingConsole", FormMethod.Post))
{
    SelectList parkingSelectList = new SelectList(Model.ParkingList, "Name", "Name");
@Html.DropDownListFor(m => m.Parking, parkingSelectList)
@Html.TextBoxFor(m => m.RegistrationNumber)

<input type="submit" name="submit" value="arrival" />
<input type="submit" name="submit" value="departure" />
}
<div id="insert">
</div>
@Ajax.ActionLink("DoShit",
                 "PCTable",
                 new AjaxOptions { HttpMethod = "GET",
                 InsertionMode = InsertionMode.Replace,
                 UpdateTargetId = "insert" })

<script src="@Url.Content("~/Scripts/jquery-3.6.4.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.min.js")" type="text/javascript"></script>